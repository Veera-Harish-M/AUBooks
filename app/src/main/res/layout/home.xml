<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_gravity="center_horizontal|center_vertical">
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#D500F9"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/cse"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/itt"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/electronics"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview4"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/electrical"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview7"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/mech"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview5"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/civil"
				android:scaleType="centerInside"/>
			<ImageView
				android:id="@+id/imageview6"
				android:layout_width="wrap_content"
				android:layout_height="150dp"
				android:layout_margin="10dp"
				android:src="@drawable/ice"
				android:scaleType="centerInside"/>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
